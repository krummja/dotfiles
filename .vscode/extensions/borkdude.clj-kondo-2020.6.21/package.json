{
	"name": "clj-kondo",
	"description": "Clj-kondo language server and client bundled in one extension.",
	"author": "Michiel Borkent",
	"license": "EPL-1",
	"version": "2020.6.21",
	"publisher": "borkdude",
	"repository": {
		"type": "git",
		"url": "https://github.com/borkdude/clj-kondo.lsp"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:clojure"
	],
	"icon": "icon.png",
