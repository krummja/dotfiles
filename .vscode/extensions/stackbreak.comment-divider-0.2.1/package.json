{
	"name": "comment-divider",
	"displayName": "Comment Divider",
	"description": "Divide your code by sections with styled separators.",
	"version": "0.2.1",
	"publisher": "stackbreak",
	"license": "GPL-3.0",
	"icon": "img/logo_256.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/stackbreak/comment-divider"
	},
	"bugs": {
		"url": "https://github.com/stackbreak/comment-divider/issues"
	},
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"keywords": [
		"comment",
		"separator",
		"divider",
		"section"
	],
	"activationEvents": [
		"onCommand:comment-divider.makeSubHeader",
		"onCommand:comment-divider.makeMainHeader",
		"onCommand:comment-divider.insertSolidLine"
	],
	"contributes": {
		"commands": [
			{
				"command": "comment-divider.makeSubHeader",
				"title": "Comment Divider: Make Subheader"
			},
			{
				"command": "comment-divider.makeMainHeader",
				"title": "Comment Divider: Make Main Header"
			},
			{
				"command": "comment-divider.insertSolidLine",
				"title": "Comment Divider: Insert Solid Line"
			}
		],
		"keybindings": [
			{
				"command": "comment-divider.makeSubHeader",
				"key": "alt+x",
				"when": "editorTextFocus"
			},
			{
				"command": "comment-divider.makeMainHeader",
				"key": "shift+alt+x",
				"when": "editorTextFocus"
			},
			{
				"command": "comment-divider.insertSolidLine",
				"key": "alt+y",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Comment Divider",
			"properties": {
				"comment-divider.length": {
					"type": "number",
					"default": 80,
					"description": "Set line length for all dividers."
				},
				"comment-divider.mainHeaderFiller": {
					"type": "string",
					"minLength": 1,
					"maxLength": 1,
					"default": "-",
					"description": "Set symbol for main header line filling (only one)."
				},
				"comment-divider.mainHeaderHeight": {
					"type": "string",
					"enum": [
						"block",
						"line"
					],
					"default": "block",
					"description": "Set main header vertical style."
				},
				"comment-divider.mainHeaderAlign": {
					"type": "string",
					"enum": [
						"center",
						"left",
						"right"
					],
					"default": "center",
					"description": "Set main header text align."
				},
				"comment-divider.mainHeaderTransform": {
					"type": "string",
					"enum": [
						"uppercase",
						"lowercase",
						"titlecase",
						"none"
					],
					"default": "none",
					"description": "Set main header text transform style."
				},
				"comment-divider.subheaderFiller": {
					"type": "string",
					"minLength": 1,
					"maxLength": 1,
					"default": "-",
					"description": "Set symbol for subheader line filling (only one)."
				},
				"comment-divider.subheaderHeight": {
					"type": "string",
					"enum": [
						"block",
						"line"
					],
					"default": "line",
					"description": "Set subheader vertical style."
				},
				"comment-divider.subheaderAlign": {
					"type": "string",
					"enum": [
						"center",
						"left",
						"right"
					],
					"default": "center",
					"description": "Set subheader text align."
				},
				"comment-divider.subheaderTransform": {
					"type": "string",
					"enum": [
						"uppercase",
						"lowercase",
						"titlecase",
						"none"
					],
					"default": "none",
					"description": "Set subheader text transform style."
				},
				"comment-divider.lineFiller": {
					"type": "string",
					"minLength": 1,
					"maxLength": 1,
					"default": "-",
					"description": "Set symbol for solid line filling."
				}
			}
		}
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"typescript": "^3.2.2",
		"typescript-tslint-plugin": "^0.3.1",
		"vscode": "^1.1.26"
	},
	"__metadata": {
		"id": "75bb3a50-2138-4144-8997-5a630e3ad890",
		"publisherId": "d17a56f4-cde4-44ad-b7b4-f3c84fe1b4ff",
		"publisherDisplayName": "stackbreak"
	}
}